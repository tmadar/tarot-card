{
    "extends": "airbnb",
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "requireConfigFile": false
    },
    "rules": {
        "indent": ["error", "tab"],
        "no-tabs": "off",
        "import/prefer-default-export": "off",
        "no-console": "off",
        "import/extensions": [
            "error",
            "ignorePackages",
            {
              "js": "always"
            }
          ]
     }
}
